$(":button").click(function() {
    var isbn = this.id;
    alert('About to report lost on ISBN ' + isbn);
    $.ajax({
  	  url: 'http://localhost:8001/library/v1/books',
  	  type: 'PUT',
  	  contentType: "application/json",
  	  data: "status=lost",
  	  success: 
  	  error: errorAlert
  });
});


